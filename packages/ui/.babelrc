{
  "presets": [
    "@babel/env",
    "@babel/react",
    "@babel/preset-typescript"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "styled-jsx/babel",
          {
            "plugins": [
              [
                "styled-jsx-plugin-sass",
                {
                  "sassOptions": {
                    "includePaths": [
                      "../../node_modules",
                      "./src/scss"
                    ]
                  }
                }
              ]
            ]
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "styled-jsx/babel-test"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "cwd": "babelrc",
        "alias": {
          "services": "./src/services"
        }
      }
    ]
  ]
}
